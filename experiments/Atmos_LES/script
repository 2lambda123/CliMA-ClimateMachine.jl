for i in 1 2 3 4 6 8 10 12
do
    env TICTOC_PRINT_RESULTS=1 CLIMA_LARGE_DYCOMS=true DISABLE_GPU=true CLIMA_STRONG_SCALING=true mpirun -np $i ~/builds/julia-1.3/julia-1.3.1/bin/julia --project=../../ dycoms.jl
    cat timings.txt >> timings_mw_large_strong.txt
    rm timings.txt
    env TICTOC_PRINT_RESULTS=1 CLIMA_LARGE_DYCOMS=false DISABLE_GPU=true CLIMA_STRONG_SCALING=true mpirun -np $i ~/builds/julia-1.3/julia-1.3.1/bin/julia --project=../../ dycoms.jl
    cat timings.txt >> timings_mw_small_strong.txt
    rm timings.txt
    env TICTOC_PRINT_RESULTS=1 CLIMA_LARGE_DYCOMS=true DISABLE_GPU=true CLIMA_STRONG_SCALING=false mpirun -np $i ~/builds/julia-1.3/julia-1.3.1/bin/julia --project=../../ dycoms.jl
    cat timings.txt >> timings_mw_large_weak.txt
    rm timings.txt
    env TICTOC_PRINT_RESULTS=1 CLIMA_LARGE_DYCOMS=false DISABLE_GPU=true CLIMA_STRONG_SCALING=false mpirun -np $i ~/builds/julia-1.3/julia-1.3.1/bin/julia --project=../../ dycoms.jl
    cat timings.txt >> timings_mw_small_weak.txt
    rm timings.txt
done
